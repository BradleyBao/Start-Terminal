{
  "onLoadMessage": {
    "text": "Git Aliases plugin loaded.",
    "type": "success"
  },
  "commands": {
    "ga": {
      "type": "alias",
      "exec": "git add .",
      "manual": "Alias for 'git add .'"
    },
    "gc": {
      "type": "alias_with_args",
      "exec": "git commit -m \"$*\"",
      "manual": "Alias for 'git commit -m <message>'"
    },
    "gp": {
      "type": "alias",
      "exec": "git push",
      "manual": "Alias for 'git push'"
    }
  }
}